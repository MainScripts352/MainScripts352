--Sent Button
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextButton100 = Instance.new("TextButton")


ScreenGui.Parent = game.CoreGui

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.new(0,0,0)
Frame.BorderColor3 = Color3.new(1,1,1)
Frame.Position = UDim2.new(0.001,0,0)
Frame.Size = UDim2.new(0.09,0,0.09)
Frame.Active = true
Frame.Draggable = true

TextButton100.Parent = Frame
TextButton100.BackgroundColor3 = Color3.new(1,1,1)
TextButton100.BackgroundTransparency = 0
TextButton100.Position = UDim2.new(0.103524067, 0, 0.200333327, 0)
TextButton100.Size = UDim2.new(0.8,0.9,0.6)
TextButton100.Font = Enum.Font.SourceSansLight
TextButton100.FontSize = Enum.FontSize.Size14
TextButton100.Text = "Sent"
TextButton100.TextScaled = true
TextButton100.TextSize = 8
TextButton100.TextWrapped = true

TextButton100.MouseButton1Click:connect(function()
if TextButton1.Text == "" then else
wait(TextButton1.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton0.text ,"All")
end
if TextButton3.Text == "" then else
wait(TextButton3.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton2.text ,"All")
end
if TextButton5.Text == "" then else
wait(TextButton5.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton4.text ,"All")
end
if TextButton7.Text == "" then else
wait(TextButton7.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton6.text ,"All")
end
if TextButton8.Text == "" then else
wait(TextButton8.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton9.text ,"All")
end
if TextButton10.Text == "" then else
wait(TextButton10.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton11.text ,"All")
end
if TextButton12.Text == "" then else
wait(TextButton12.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton13.text ,"All")
end
if TextButton14.Text == "" then else
wait(TextButton14.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton15.text ,"All")
end
if TextButton16.Text == "" then else
wait(TextButton16.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton17.text ,"All")
end
if TextButton18.Text == "" then else
wait(TextButton18.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton19.text ,"All")
end
if TextButton21.Text == "" then else
wait(TextButton21.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton21.text ,"All")
end
if TextButton22.Text == "" then else
wait(TextButton22.text)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextButton23.text ,"All")
end
end)
