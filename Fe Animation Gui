OpenGui = Instance.new("ScreenGui") 
OpenGui.Parent = game.CoreGui

local Open = Instance.new("TextButton") 
Open.Parent = OpenGui
Open.BackgroundColor3 = Color3.fromRGB(242,243,243)
Open.BackgroundTransparency = 0
Open.Position = UDim2.new(0.8,0,0)
Open.Size = UDim2.new(0.13,0,0.13)
Open.Font = Enum.Font.Cartoon
Open.TextColor3 = Color3.fromRGB(17,17,17)
Open.Text = "Animation"
Open.TextSize = 18
Open.TextScaled = true
Open.TextWrapped = false
Open.Active = true
Open.Draggable = false

Open.MouseButton1Click:Connect(function() 
if Frame1.Visible == false then
   Frame1.Visible = true
else
   Frame1.Visible = false
end
Open.BackgroundColor3 = Color3.fromRGB(171, 173, 172)
wait()
Open.BackgroundColor3 = Color3.fromRGB(242,243,243)
end)

SaveButton.MouseButton1Click:Connect(function()
RequestDialog("Script","Are you sure \
do you want to save this animation","Cancel","Save","",'SaveAnimation() RequestToastMessage("Saved!")')
SaveButton.BackgroundColor3 = Color3.fromRGB(40, 127, 71)
wait(0.1)
SaveButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
end)

LoadButton.MouseButton1Click:Connect(function()
LoadButton.BackgroundColor3 = Color3.fromRGB(40, 127, 71)
wait(0.1)
LoadButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
if isfile("Animation") then
   RequestDialog("Script","Are you sure do you want to \nload your saved animation","Cancel","Load","","loadstring(readfile('Animation'))() RequestToastMessage('Animation Successfully Loaded!')")
else
   RequestToastMessage("You dont have saved Animation!")
end
end)


CloseButton.MouseButton1Click:Connect(function()
RequestDialog("Script","Are you sure \
do you want to kill the gui","Cancel","Yes","","ScreenGui1:Destroy() OpenGui:Destroy()")
CloseButton.BackgroundColor3 = Color3.fromRGB(123, 46, 47)
wait(0.1)
CloseButton.BackgroundColor3 = Color3.fromRGB(255,0,0)
end)




