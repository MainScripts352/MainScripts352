function RequestDialog(title,message,button1,button2,script1,script2)
 local DialogGui = Instance.new("ScreenGui")
 local DialogFrame = Instance.new("Frame")
 local DialogFrame2 = Instance.new("Frame")
 local DialogLine = Instance.new("Frame")
 local DialogTitle = Instance.new("TextLabel")
 local DialogMessage = Instance.new("TextLabel")
 local DialogButton1 = Instance.new("TextButton")
 local DialogButton2 = Instance.new("TextButton")
 local DialogUICorner = Instance.new("UICorner")

 DialogGui.Parent = game.CoreGui

 DialogFrame.Parent = DialogGui
 DialogFrame.BackgroundColor3 = Color3.new(0,0,0)
 DialogFrame.Transparency = 0.5
 DialogFrame.BorderColor3 = Color3.new(1,1,1)
 DialogFrame.BorderSizePixel = 0
 DialogFrame.Position = UDim2.new(0,0,-0.2)
 DialogFrame.Size = UDim2.new(1,0,1.2)
 DialogFrame.Active = true
 DialogFrame.Draggable = false

 DialogFrame2.Parent = DialogFrame
 DialogFrame2.BackgroundColor3 = Color3.fromRGB(35,35,35)
 DialogFrame2.BorderColor3 = Color3.new(1,1,1)
 DialogFrame2.BorderSizePixel = 0
 DialogFrame2.Position = UDim2.new(0.35,0,0.35)
 DialogFrame2.Size = UDim2.new(0.33,0,0.35)
 DialogFrame2.Active = true
 DialogFrame2.Draggable = false

 DialogUICorner.CornerRadius = UDim.new(0.1, 0)
 DialogUICorner.Parent = DialogFrame2

 DialogLine.Parent = DialogFrame2
 DialogLine.BackgroundColor3 = Color3.new(0,0,0)
 DialogLine.BorderColor3 = Color3.new(1,1,1)
 DialogLine.BorderSizePixel = 2
 DialogLine.Position = UDim2.new(0.1,0,0.26)
 DialogLine.Size = UDim2.new(0.78,0,0.0000001)
 DialogLine.Active = true

 DialogTitle.Parent = DialogFrame2
 DialogTitle.BackgroundColor3 = Color3.fromRGB(35,35,35)
 DialogTitle.BackgroundTransparency = 1
 DialogTitle.Position = UDim2.new(0.1,0,0.05)
 DialogTitle.Text = ""
 DialogTitle.TextColor3 = Color3.new(1,1,1)
 DialogTitle.Size = UDim2.new(0.8,0,0.15)
 DialogTitle.FontSize = Enum.FontSize.Size14
 DialogTitle.TextScaled = false
 DialogTitle.TextSize = 16
 DialogTitle.TextWrapped = true

 DialogMessage.Parent = DialogFrame2
 DialogMessage.BackgroundColor3 = Color3.new(1,1,1)
 DialogMessage.BackgroundTransparency = 1
 DialogMessage.Position = UDim2.new(0.1,0,0.35)
 DialogMessage.Text = ""
 DialogMessage.TextColor3 = Color3.new(1,1,1)
 DialogMessage.Size = UDim2.new(0.79,0,0.2)
 DialogMessage.FontSize = Enum.FontSize.Size14
 DialogMessage.TextScaled = true
 DialogMessage.TextSize = 14
 DialogMessage.TextWrapped = true

 DialogButton1.Parent = DialogFrame2
 DialogButton1.BackgroundColor3 = Color3.new(1,1,1)
 DialogButton1.BackgroundTransparency = 0
 DialogButton1.Position = UDim2.new(0.1,0,0.65)
 DialogButton1.Text = ""
 DialogButton1.TextColor3 = Color3.new(0,0,0)
 DialogButton1.Size = UDim2.new(0.37,0,0.2)
 DialogButton1.FontSize = Enum.FontSize.Size14
 DialogButton1.TextScaled = true
 DialogButton1.TextSize = 8
 DialogButton1.TextWrapped = true

 DialogButton2.Parent = DialogFrame2
 DialogButton2.BackgroundColor3 = Color3.new(1,1,1)
 DialogButton2.BackgroundTransparency = 0
 DialogButton2.Position = UDim2.new(0.52,0,0.65)
 DialogButton2.Text = ""
 DialogButton2.TextColor3 = Color3.new(0,0,0)
 DialogButton2.Size = UDim2.new(0.37,0,0.2)
 DialogButton2.FontSize = Enum.FontSize.Size14
 DialogButton2.TextScaled = true
 DialogButton2.TextSize = 8
 DialogButton2.TextWrapped = true

 DialogButton1.MouseButton1Click:connect(function()
 DialogGui:Destroy()
 loadstring(script1)()
 end)

 DialogButton2.MouseButton1Click:connect(function()
 DialogGui:Destroy()
 loadstring(script2)()
 end)
 
 if title then
   DialogTitle.Text = ""..title
 end
 if message then
   DialogMessage.Text = ""..message
 end
 if button1 then
   DialogButton1.Text = ""..button1
 end
 if button2 then
   DialogButton2.Text = ""..button2
 end
end


