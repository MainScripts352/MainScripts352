local CoreGui = game:GetService("StarterGui")

CoreGui:SetCore("SendNotification", {
    Title = "Script Made By",
    Text = "GhostPlayer",
    Icon = "rbxassetid://29819383",
    Duration = 2.5,
})

--Admin Detector in Server
loadstring(game:HttpGet('https://raw.githubusercontent.com/MainScripts352/MainScripts352/main/Admin'))()
--

--[[ Request Toast Message Function
RequestToastMessage("Hello World", 2) --Use this to request toast message notification
]]
loadstring(game:HttpGet("https://raw.githubusercontent.com/MainScripts352/MainScripts352/refs/heads/main/Request Toast Message"))()
--


---For ReExecute Detector Operator
local ExecutedValue = 1
if not workspace:FindFirstChild("Fe Animation Gui") then
   --Starter Execute
   local String = Instance.new("NumberValue")
   String.Parent = game.workspace
   String.Name = "Fe Animation Gui"
   String.Archivable = true
   String.Value = "1"

   PickedBundle = nil --(Picked Animation Bundle)
   PickedAnimate = nil --(Picked Animate Type)

   IdleAnimate = nil
   Idle2Animate = nil
   WalkAnimate = nil
   RunAnimate = nil
   JumpAnimate = nil
   ClimbAnimate = nil
   FallAnimate = nil
   SwimIdleAnimate = nil
   SwimAnimate = nil
else
   workspace:FindFirstChild("Fe Animation Gui").Value += 1
   ExecutedValue = workspace:FindFirstChild("Fe Animation Gui").Value
end
--



---Looping Animation
game:GetService('RunService').RenderStepped:connect(function()
if workspace:FindFirstChild("Fe Animation Gui").Value == ExecutedValue then
   if IdleAnimate ~= nil and Idle2Animate ~= nil then
      game.Players.LocalPlayer.Character.Animate.idle.Animation1.AnimationId = IdleAnimate
      game.Players.LocalPlayer.Character.Animate.idle.Animation2.AnimationId = Idle2Animate
   end
   if WalkAnimate ~= nil then
      if game.Players.LocalPlayer.Character.Animate.walk:FindFirstChild("WalkAnim") then
         game.Players.LocalPlayer.Character.Animate.walk.WalkAnim.AnimationId = WalkAnimate
      else
         game.Players.LocalPlayer.Character.Animate.walk.walk.AnimationId = WalkAnimate
      end
   end
   if RunAnimate ~= nil then
      if game.Players.LocalPlayer.Character.Animate.run:FindFirstChild("RunAnim") then
         game.Players.LocalPlayer.Character.Animate.run.RunAnim.AnimationId = RunAnimate
      else
         game.Players.LocalPlayer.Character.Animate.run.run.AnimationId = RunAnimate
      end
   end
   if JumpAnimate ~= nil then
      if game.Players.LocalPlayer.Character.Animate.jump:FindFirstChild("JumpAnim") then
         game.Players.LocalPlayer.Character.Animate.jump.JumpAnim.AnimationId = JumpAnimate
      else
         game.Players.LocalPlayer.Character.Animate.jump.jump.AnimationId = JumpAnimate
      end
   end
   if ClimbAnimate ~= nil then
      game.Players.LocalPlayer.Character.Animate.climb.ClimbAnim.AnimationId = ClimbAnimate
   end
   if FallAnimate ~= nil then
      game.Players.LocalPlayer.Character.Animate.fall.FallAnim.AnimationId = FallAnimate
   end
   if SwimIdleAnimate ~= nil and SwimAnimate ~= nil then
      game.Players.LocalPlayer.Character.Animate.swimidle.SwimIdle.AnimationId = SwimIdleAnimate
      game.Players.LocalPlayer.Character.Animate.swim.Swim.AnimationId = SwimAnimate
   end
end
end)
---



---Auto Load Animation Mode Function
--AutoLoadAnimation = true --use this to auto load your saved animation without showing the gui
if AutoLoadAnimation == nil then
   AutoLoadAnimation = false
end

if AutoLoadAnimation == true then
   pcall(function()
   if isfile('Animation') then
      loadstring(readfile('Animation'))() 
      RequestToastMessage('Animation Successfully Loaded!')
      while true do
        task.wait()
      end
   else
      RequestToastMessage("You dont have saved Animation!")
   end
   end)
end
---




---Picked Animation Operator
