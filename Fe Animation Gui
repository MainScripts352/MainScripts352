---


function SaveAnimation()
 writefile('Animation','AnimationFile = true\
 ')
 if IdleAnimate ~= nil then
    writefile('Animation','IdleAnimate = "'..IdleAnimate..'"')
 end
 if Idle2Animate ~= nil then
    writefile('Animation',readfile('Animation')..' Idle2Animate = "'..Idle2Animate..'"')
 end
 if WalkAnimate ~= nil then
    writefile('Animation',readfile('Animation')..' WalkAnimate = "'..WalkAnimate..'"')
 end
 if RunAnimate ~= nil then
    writefile('Animation',readfile('Animation')..' RunAnimate = "'..RunAnimate..'"')
 end
 if JumpAnimate ~= nil then
    writefile('Animation',readfile('Animation')..' JumpAnimate = "'..JumpAnimate..'"')
 end
 if FallAnimate ~= nil then
    writefile('Animation',readfile('Animation')..' FallAnimate = "'..FallAnimate..'"')
 end
 if ClimbAnimate ~= nil then
    writefile('Animation',readfile('Animation')..' ClimbAnimate = "'..ClimbAnimate..'"')
 end
 if SwimIdleAnimate ~= nil then
    writefile('Animation',readfile('Animation')..' SwimIdleAnimate = "'..SwimIdleAnimate..'"')
 end
 if SwimAnimate ~= nil then
    writefile('Animation',readfile('Animation')..' SwimAnimate = "'..SwimAnimate..'"')
 end
end




local function RequestList()
 AnimationGui = Instance.new("ScreenGui")
 AnimationFrame = Instance.new("Frame")
 AnimationScrolling = Instance.new("ScrollingFrame")
 CancelButton = Instance.new("TextButton") 
 AnimationGui.Parent = game.CoreGui

 AnimationFrame.Parent = AnimationGui
 AnimationFrame.BackgroundColor3 = Color3.new(0,0,0)
 AnimationFrame.Transparency = 0.5
 AnimationFrame.BorderColor3 = Color3.new(1,1,1)
 AnimationFrame.BorderSizePixel = 0
 AnimationFrame.Position = UDim2.new(0,0,-0.2)
 AnimationFrame.Size = UDim2.new(1,0,1.2)
 AnimationFrame.Active = true
 AnimationFrame.Draggable = false

 AnimationScrolling.Parent = AnimationFrame
 AnimationScrolling.BackgroundColor3 = Color3.new(0,0,0)
 AnimationScrolling.BackgroundTransparency = 0.8
 AnimationScrolling.Position = UDim2.new(0, 0, 0)
 AnimationScrolling.Size = UDim2.new(1,0,1)
 AnimationScrolling.CanvasSize = UDim2.new(100, 0, 0, 0)
 AnimationScrolling.ScrollBarThickness = 0
 AnimationScrolling.Visible = true
 
 CancelButton.Parent = AnimationGui
 CancelButton.BackgroundColor3 = Color3.fromRGB(242,243,243)
 CancelButton.BackgroundTransparency = 0
 CancelButton.Position = UDim2.new(0.8,0,0)
 CancelButton.Size = UDim2.new(0.13,0,0.13)
 CancelButton.Font = Enum.Font.Cartoon
 CancelButton.TextColor3 = Color3.fromRGB(17,17,17)
 CancelButton.Text = "Cancel"
 CancelButton.TextSize = 18
 CancelButton.TextScaled = true
 CancelButton.TextWrapped = false
 CancelButton.Active = true
 CancelButton.Draggable = false
 
 CancelButton.MouseButton1Click:Connect(function()
 AnimationGui:Destroy()
 end)
end

local UIButtonPosition = 0
local function AddButton(image,name,script)
 local Image = Instance.new("ImageButton")
 Image.Parent = AnimationScrolling
 Image.Transparency = 0
 Image.Position = UDim2.new(UIButtonPosition, 0, 0.35, 0)
 Image.Size = UDim2.new(0.002, 0, 0.3)
 Image.Image = ""..image

 local TextLabel = Instance.new("TextLabel")
 TextLabel.Parent = AnimationScrolling
 TextLabel.BackgroundColor3 = Color3.new(1,1,1)
 TextLabel.BackgroundTransparency = 0
 TextLabel.Position = UDim2.new(UIButtonPosition, 0, 0.7, 0)
 TextLabel.TextColor3 = Color3.new(0,0,0)
 TextLabel.Size = UDim2.new(0.002,0,0.1)
 TextLabel.Font = Enum.Font.SourceSansBold
 TextLabel.FontSize = Enum.FontSize.Size14
 TextLabel.Text = ""..name
 TextLabel.TextScaled = false
 TextLabel.TextSize = 20.00
 TextLabel.TextWrapped = true
 UIButtonPosition += 0.003

 Image.MouseButton1Click:Connect(function()
 AnimationGui:Destroy()
 if PickedAnimate == "All" then
    PickedBundle = ""..name
    SetAnimate()
 elseif PickedAnimate == "Idle" then
    PickedBundle = string.gsub(name, " Idle", "")
    PickedAnimate = ""..name
 elseif PickedAnimate == "Walk" then
    PickedBundle = string.gsub(name, " Walk", "")
    PickedAnimate = ""..name
 elseif PickedAnimate == "Run" then
    PickedBundle = string.gsub(name, " Run", "")
    PickedAnimate = ""..name
 elseif PickedAnimate == "Jump" then
    PickedBundle = string.gsub(name, " Jump", "")
    PickedAnimate = ""..name
 elseif PickedAnimate == "Climb" then
    PickedBundle = string.gsub(name, " Climb", "")
    PickedAnimate = ""..name
 elseif PickedAnimate == "Fall" then
    PickedBundle = string.gsub(name, " Fall", "")
    PickedAnimate = ""..name
 elseif PickedAnimate == "Swim" then
    PickedBundle = string.gsub(name, " Swim", "")
    PickedAnimate = ""..name
 end
 loadstring(script)()
 end)
end


