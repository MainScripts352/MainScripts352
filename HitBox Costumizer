TextButton9.MouseButton1Click:connect(function()

_G.Transparency = TextBox.Text --1 to 0.9
_G.HeadSize = TextBox1.Text -- Size of hitbox
_G.BrickColor = "Really "..TextBox3.Text -- color,Really black,Really red,Really blue,Really white
_G.Material = TextBox2.Text --Neon,Brick,Plastic,
_G.Disabled = true

game:GetService('RunService').RenderStepped:connect(function()
if _G.Disabled then
for i,v in next, game:GetService('Players'):GetPlayers() do
if v.Name ~= game:GetService('Players').LocalPlayer.Name then
pcall(function()
v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
v.Character.HumanoidRootPart.Transparency = _G.Transparency,_G.Transparency,_G.Transparency
v.Character.HumanoidRootPart.BrickColor = BrickColor.new(_G.BrickColor)
v.Character.HumanoidRootPart.Material = _G.Material
v.Character.HumanoidRootPart.CanCollide = false
end)
end
end
end
end)
end)
