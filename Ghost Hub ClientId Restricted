--[[ Restricted Client ID Detector for Ghosthub MoreScript
local Check = ClientIsRestricted(Client ID Here) --use this to check if client id is restricted
]]

local RestrictedId = {
  "4bc4336e-26d4-48b4-84f9-1ab154fa113f",
  "44c9083b-fbf3-4745-a68f-82fe0213c854",
  "f01c4745-db64-4ece-8a1d-dd215cda93f3",
  "a2bcdd22-e266-4ac8-9b90-0b6d2f33fe36",
  "b5f48327-37e7-4640-bb24-b2a5299d26c3",
  "adb41a30-8b49-4ec2-b559-5f6bc460d60c",
  "7BD0A1AC-69DF-46EC-A447-2741A90E6502"
}

function ClientIsRestricted(clientId)
 for key, value in pairs(RestrictedId) do
   if clientId == value then
      return true
   end
 end
 return false
end
