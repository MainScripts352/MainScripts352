--[[ Restricted Scripts Detector for Ghosthub MoreScript
local Check = ScriptRestricted("Script Here") --use this to check if script is restricted
]]

local RestrictedScripts = {
  "https://raw.githubusercontent.com/IamDavidGod/",
  "https://dpaste.com/6G3Z99U2P.txt",
  "https://raw.githubusercontent.com/f4shn/",
  "https://raw.githubusercontent.com/dandzoboss/",
  "https://rawscripts.net/raw/Universal-Script-Fire-Parts-for-obbys-33266",
  "https://raw.githubusercontent.com/SCRlPT-HUB/AdoptMe_Script/refs/heads/main/AdoptMe_PetDuplicator",
  "https://raw.githubusercontent.com/EnesXVC/FireParts/main/Script",
  "https://raw.githubusercontent.com/vhigiyfhh/",
  "https://raw.githubusercontent.com/TraxMain/",
  "https://raw.githubusercontent.com/l/",
  "https://raw.githubusercontent.com/sinsonur53/",
  "https://raw.githubusercontent.com/214Eli/",
  "http://vpaste.net/8IMWf",
  "http://vpaste.net/qXeVA"
}

function ScriptRestricted(script)
 for _, value in ipairs(RestrictedScripts) do
   if string.find(script, value, 1, true) then
      return true
   end
 end
 return false
end
