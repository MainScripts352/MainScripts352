--[[ Restricted Scripts Detector for Ghosthub MoreScript
local Check = ScriptRestricted("Script Here") --use this to check if script is restricted
]]

local RestrictedScripts = {
  "loadstring(game:HttpGet('https://raw.githubusercontent.com/f4shn/websocket/refs/heads/main/my_script.lua',true))()"
}

function ScriptRestricted(script)
 for key, value in pairs(RestrictedScripts) do
   if value == script then
      return true
   end
 end
 return false
end
